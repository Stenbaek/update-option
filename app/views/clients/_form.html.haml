= form_for @client do |f|
  = render "shared/error_messages", :target => @client
  
  %fieldset
    %p
      = f.label :name
      = f.text_field :name
      
    - f.fields_for :websites do |w|
      = w.label :webaddress
      = w.text_field :webaddress
    
    %p
      = f.submit 'Save'
      = cancel_link
